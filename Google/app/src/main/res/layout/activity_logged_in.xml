<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.akshay.google.LoggedIn">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_upload"
        android:src="@drawable/ic_file_upload_white_24dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton_add"
        android:layout_toEndOf="@+id/imageButton_add"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_refresh"
        android:layout_alignBottom="@+id/imageButton_upload"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageButton_upload"
        android:src="@drawable/ic_refresh_white_24dp"/>

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_add"
        android:src="@drawable/ic_person_add_white_24dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton_refresh"
        android:layout_toEndOf="@+id/imageButton_refresh"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton_profile"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/imageButton_upload"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/ic_more_horiz_white_24dp"/>


</RelativeLayout>
